<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="460.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.addressbook.VideoController">
   <children>
      <MediaView fx:id="mediaView" fitHeight="230.0" fitWidth="450.0" layoutX="175.0" layoutY="25.0" />
      <Button fx:id="playButton" layoutX="145.0" layoutY="320.0" mnemonicParsing="false" onAction="#playButton_method" prefHeight="40.0" prefWidth="115.0" text="PLAY">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="pauseButton" layoutX="324.0" layoutY="320.0" mnemonicParsing="false" onAction="#pauseButton_method" prefHeight="40.0" prefWidth="115.0" text="PAUSE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="503.0" layoutY="320.0" mnemonicParsing="false" onAction="#resetButton_method" prefHeight="40.0" prefWidth="115.0" text="RESET">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnAudio" layoutX="579.0" layoutY="404.0" mnemonicParsing="false" onAction="#audioPlayer" prefHeight="30.0" prefWidth="102.0" text="Audio Player" />
   </children>
</AnchorPane>
